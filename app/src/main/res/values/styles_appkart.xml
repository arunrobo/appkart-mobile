<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="EditTextappkart" parent="android:Widget.EditText">
	  <item name="android:background">@drawable/appkart_edit_text_holo_light</item>
	  <item name="android:textColor">#808080</item>
  </style>
   <style name="TextView">
       <item name="android:textColor">#808080</item>
   </style>
  <style name="AutoCompleteTextViewappkart" parent="android:Widget.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/appkart_list_selector_holo_light</item>
	  <item name="android:background">@drawable/appkart_edit_text_holo_light</item>
	  <item name="android:textColor">#808080</item>
  </style>

  <style name="CheckBoxappkart" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/appkart_btn_check_holo_light</item>
  </style>

  <style name="RadioButtonappkart" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/appkart_btn_radio_holo_light</item>
  </style>

  <style name="Buttonappkart" parent="android:Widget.Button">
	  <item name="android:background">@drawable/appkart_btn_default_holo_light</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#808080</item>
  </style>

  <style name="ImageButtonappkart" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/appkart_btn_default_holo_light</item>
  </style>

  <style name="Spinnerappkart" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/appkart_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/appkart_list_selector_holo_light</item>
  </style>

  <style name="SpinnerDropDownItemappkart" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/appkart_btn_radio_holo_light</item>
  </style>
  <style name="Tabappkart">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/appkart_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextappkart">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarappkart" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/appkart_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/appkart_progress_indeterminate_horizontal_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBarappkart" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/appkart_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/appkart_scrubber_progress_horizontal_holo_light</item>
      <item name="android:minHeight">13dip</item>
      <item name="android:maxHeight">13dip</item>
      <item name="android:thumb">@drawable/appkart_scrubber_control_selector_holo_light</item>
      <item name="android:thumbOffset">16dip</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
  </style>

  <style name="RatingBarappkart" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/appkart_ratingbar_full_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/appkart_ratingbar_full_holo_light</item>
  </style>

  <style name="RatingBarBigappkart" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/appkart_ratingbar_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/appkart_ratingbar_holo_light</item>
      <item name="android:minHeight">35dip</item>
      <item name="android:maxHeight">35dip</item>
  </style>

  <style name="RatingBarSmallappkart" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/appkart_ratingbar_small_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/appkart_ratingbar_small_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="Toggleappkart" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/appkart_btn_toggle_holo_light</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">#808080</item>
  </style>

  <style name="ListViewappkart" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/appkart_list_selector_holo_light</item>
  </style>

  <style name="ListViewappkart.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/appkart_list_selector_holo_light</item>
  </style>

  <style name="SpinnerItemappkart" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">#808080</item>
  </style>

</resources>